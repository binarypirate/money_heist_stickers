<?xml version="1.0" encoding="utf-8"?>
<resources>
	<!-- Content description for the rose pack tray image -->
	<string name="tray_image_content_description">Слика палете пакета налепница</string>
	<!-- the title of info button in the menu that takes the user to more information about that pack showing publisher website and so on -->
	<string name="more_info_button">Више информација о пакету налепница</string>
	<!-- Text to be shown on the button that user can click to add the rose pack to WhatsApp -->
	<string name="add_to_whatsapp">Додај у WhatsApp</string>
	<!-- Content Description for the plus button shown on rose pack list, that allows user to add rose pack to WhatsApp -->
	<string name="add_button_content_description">Додај пакет налепница у WhatsApp</string>
	<!-- Title of the rose pack page when there are multiple rose packs in the app -->
	<string name="title_activity_sticker_pack_details_multiple_pack">Детаљи налепнице</string>
	<!-- Title of the rose pack info page which displays the webpage, email and privacy policy of the rose pack -->
	<string name="title_activity_sticker_pack_info">Информације</string>
	<!-- Text of button that allows user to click to go to the webpage of the publisher -->
	<string name="view_webpage">Погледај веб страницу</string>
	<!-- Text of button that allows user to click to send an email -->
	<string name="send_email">Пошаљи имејл</string>
	<!-- Text of button that allows user to click to go to the privacy policy website -->
	<string name="privacy_policy">Политика приватности</string>
	<!-- Text next to the tray icon that indicates the tray icon -->
	<string name="info_screen_tray_icon">Икона палете</string>
	<!-- Text shown to the user instead of Add to WhatsApp button if the pack is already added to WhatsApp -->
	<string name="details_pack_already_added">Пакет налепница је додат у WhatsApp</string>
	<!-- Title in the intent chooser that shows when user has multiple email client installed -->
	<string name="info_send_email_to_prompt">Пошаљи имејл са</string>
	<!-- Message to show in a dialog when user fails to add rose pack -->
	<string name="add_pack_fail_prompt_update_whatsapp">Пакет налепница није додат. Ако желиш да га додаш, обавезно ажурирај на најновију верзију WhatsApp-а.</string>
	<!-- Text in dialog button when a user has an old version of WhatsApp that prevents them from adding the rose pack, clicking on it will take user to play store -->
	<string name="add_pack_fail_prompt_update_play_link">Ажурирај</string>
	<!-- Toast text when we could not take user to play store because they don't have Google Play app installed -->
	<string name="cannot_find_play_store">Google Play није инсталиран на овом уређају.</string>
	<!-- Text of button that allows user to click to go to the license agreement website -->
	<string name="license_agreement">Уговор о лиценцирању</string>
	<!-- Title of the rose pack list activity page in case the app contains multiple rose packs, the singular string will be used when there is only one pack in the app -->
	<plurals name="title_activity_sticker_packs_list">
		<item quantity="one">Пакет налепница</item>
		<item quantity="other">Пакети налепница</item>
	</plurals>
</resources>
